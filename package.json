{
  "name": "react-mui-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist ./node_modules",
    "dev": "(kill -9 $(lsof -t -i:1878) || true) && vite --port 1878",
    "start": "vite --port 1878 --strictPort",
    "build": "tsc -b && vite build && ANALYZE=true",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,mts,d.ts}\" --no-error-on-unmatched-pattern",
    "prepare": "husky",
    "preview": "npm run build && wrangler pages dev ./dist",
    "deploy": "npm run build && wrangler pages deploy ./dist",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.12.10",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.3",
    "@mui/material": "^6.4.3",
    "@mui/x-data-grid": "^7.26.0",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "jwt-check-expiry": "^1.0.10",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-pro-sidebar": "^1.1.0",
    "react-router-dom": "^7.1.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-react": "^7.26.3",
    "@cloudflare/workers-types": "^4.20250204.0",
    "@eslint/js": "^9.17.0",
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/client-preset": "4.6.1",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.3",
    "@graphql-codegen/typescript-operations": "^4.4.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.1",
    "@types/parse-json": "^4.0.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.38.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "wrangler": "^3.107.3"
  }
}
